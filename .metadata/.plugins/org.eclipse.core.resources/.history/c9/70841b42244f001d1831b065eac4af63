
public abstract class Veiculo {
	private String placa;
	private int ano;
	
	
	
public Veiculo(String placa,int ano) {
	super();
	this.placa = placa;
	this.ano= ano;
	
	
}

public String getplaca() {
	return placa;
	
}

public void setplaca(String placa) {
	this.placa=placa;
	
	
}

public int getano() {
	return ano;
	
}
	
	
	public void setano (int ano)  { 
		this.ano = ano;
		
	}
	public void exibirdados() {
		
	}
		
			
}

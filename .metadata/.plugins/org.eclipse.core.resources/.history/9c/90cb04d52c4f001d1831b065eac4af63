
public abstract class Veiculo {
	private String placa;
	private int ano;
	
	
	
public Veiculo () {
	
}

public Veiculo(String placa,int ano) {
	this.placa=placa;
	this.ano=ano;
}

public abstract void exibirdados ();

public String getplaca() {
	return placa;
}
	

public void setplaca (String placa) {
	this.placa = placa;
}

}

